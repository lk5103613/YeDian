<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <!-- 顶部标题 -->

    <include
        android:id="@+id/title"
        layout="@layout/title" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@id/bottommenu"
        android:layout_below="@id/title" >

        <LinearLayout
            android:id="@+id/line_img_0"
            android:layout_width="277dip"
            android:layout_height="59dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="50dip"
            
            android:focusable="false"
            android:focusableInTouchMode="true"
            android:orientation="horizontal" >
            <TextView
                android:id="@+id/weibo"
                android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              
                android:background="@drawable/weibo"
                 
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/qq"
                 android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              android:layout_marginLeft="4dip"
                android:background="@drawable/qq"
                android:paddingLeft="53dip"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/wechat"
               android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              
                android:background="@drawable/wechat"
                android:layout_marginLeft="4dip"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/friends_quan"
                android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              android:layout_marginLeft="4dip"
                android:background="@drawable/friends_quan"
                
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="15sp" />
        </LinearLayout>
        
        <LinearLayout
            android:id="@+id/line1_text_ll"
            android:layout_width="277dip"
            android:layout_height="59dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="12dip"
             
            android:layout_below="@id/line_img_0"
            android:focusable="false"
            android:focusableInTouchMode="true"
            android:orientation="horizontal" >
            <TextView
                android:id="@+id/num1"
                android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
               android:text="新浪微博"
                android:textColor="@color/black"
                 
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="13sp" />
            <TextView
                android:id="@+id/cell_phone"
                 android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              android:layout_marginLeft="4dip"
                android:text="QQ空间"
                android:textColor="@color/black"
              android:gravity="center_horizontal"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="13sp" />
            <TextView
                android:id="@+id/cell_phone"
               android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              
                android:text="微信"
                android:textColor="@color/black"
              android:gravity="center_horizontal"
                android:layout_marginLeft="4dip"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/cell_phone"
                android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              android:layout_marginLeft="4dip"
                 android:text="微信朋友圈"
                android:textColor="@color/black"
              android:gravity="center_horizontal"
                
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="13sp" />
        </LinearLayout>
        
        
        <LinearLayout
            android:id="@+id/line_img_1"
            android:layout_width="277dip"
            android:layout_height="59dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="160dip"
            
            android:focusable="false"
            android:focusableInTouchMode="true"
            android:orientation="horizontal" >
            <TextView
                android:id="@+id/mail"
                android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              
                android:background="@drawable/mail"
                 
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/msg"
                 android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              android:layout_marginLeft="4dip"
                android:background="@drawable/msg"
                android:paddingLeft="53dip"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/copy_zy"
               android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              
                android:background="@drawable/copy_zy"
                android:layout_marginLeft="4dip"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/cell_phone"
                android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              android:layout_marginLeft="4dip"
               
                
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="15sp" />
        </LinearLayout>
        
        <LinearLayout
            android:id="@+id/line1_text1_ll"
            android:layout_width="277dip"
            android:layout_height="59dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dip"
             
            android:layout_below="@id/line_img_1"
            android:focusable="false"
            android:focusableInTouchMode="true"
            android:orientation="horizontal" >
            <TextView
                android:id="@+id/num1"
                android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
               android:text="邮件"
                android:textColor="@color/black"
                 
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="13sp" />
            <TextView
                android:id="@+id/cell_phone"
                 android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              android:layout_marginLeft="4dip"
                android:text="消息"
                android:textColor="@color/black"
              android:gravity="center_horizontal"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="13sp" />
            <TextView
                android:id="@+id/cell_phone"
               android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              
                android:text="复制正义"
                android:textColor="@color/black"
              android:gravity="center_horizontal"
                android:layout_marginLeft="4dip"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/cell_phone"
                android:layout_width="59dip"
                android:layout_height="59dip"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dip"
                android:layout_weight="1"
              android:layout_marginLeft="4dip"
                  
                android:textColor="@color/black"
              android:gravity="center_horizontal"
                
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColorHint="@color/lightgrey"
                android:textSize="13sp" />
        </LinearLayout>
        

        

        <TextView
            android:id="@+id/cancel_share"
            android:layout_width="277dip"
            android:layout_height="35dip"
            android:layout_below="@id/password_box"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="305dip"
            android:background="@drawable/cancel_share"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="20sp" />
    </RelativeLayout>

</RelativeLayout>